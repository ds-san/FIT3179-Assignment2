{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 1480,
  "background" : "#f2f9ff",
  "title": { "text": "Suicide rates in countries" },
  "data": {
  "url": "https://raw.githubusercontent.com/ds-san/FIT3179-Assignment2/main/data/continents.csv"
  },

    "mark": {
        "type": "line",
        "tooltip":true,
        "point": {
          "filled": false,
          "fill": "white"
        }
      },
      "encoding": {
        "x": {"timeUnit": "year", 
        "field": "year",
        "title":"Year"},
        "y": { "field": "cases", 
        "type": "quantitative",
        "title":"Suicides per 100,000 population",
        "scale": {"domain": [10,16]}}
      }
    }
    